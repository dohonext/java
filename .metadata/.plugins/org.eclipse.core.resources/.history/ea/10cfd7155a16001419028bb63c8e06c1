package homework3;

public class Player {

	public int level;
	public String name;
	
	public Player() {	
		new Player(1, "no name");
	}
	
	public Player(int level, String name) {
		this.level = level;
		this.name = name;
	}

	public void upgradeLevel(){
		this.level++;
	}
	
	public int getGameLevel(){
		return this.level;
	}
	
	public void attack(){
		
	}
	
}
