package company;

public class Singleton {
	
	private String CompanyName = "Hyundai";
	public static Singleton instance = new Singleton();
	private Singleton(){}
	
	public static Singleton getInstance(){
		if (instance == null){
			instance = new Singleton();
		}
		return instance;
	}
	
	public String getCompanyName(){
		return CompanyName;
	}
}
