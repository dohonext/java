package MazeSolve;

public class Maze {
	public int[][] map = new int[][]{
		{9,9,9,9,9,9,9,9,9,9},
		{9,1,9,9,9,0,9,9,9,9},
		{9,0,0,0,9,0,0,0,0,9},
		{9,9,9,0,0,0,9,0,9,9},
		{9,9,9,0,9,9,9,0,9,9},
		{9,9,0,0,9,0,0,0,0,9},
		{9,0,9,9,9,0,9,9,9,9},
		{9,9,0,9,9,0,0,0,0,9},
		{9,0,9,9,0,9,9,9,0,9},
		{9,9,9,9,9,9,9,9,9,9}
	};
	
	public void showMaze(){
		for(int i=0; i<10; i++){
			for(int j=0; j<10; j++){
				if(map[i][j] == 9){
					System.out.print("█");
				} else if(map[i][j] == 0 || map[i][j] == 2){
					System.out.println(" ");
				} else{
					System.out.println("©");
				}
				System.out.println();
				
			}
		}
	}
	
	
}
