package MazeSolve;

public class Maze {
	public int[][] maze = new int[][]{
		{9,9,9,9,9,9,9,9,9,9},
		{9,0,9,9,9,0,9,9,9,9},
		{9,0,0,0,9,0,0,0,0,9},
		{9,9,9,0,0,0,9,0,9,9},
		{9,9,9,0,9,9,9,0,9,9},
		{9,9,0,0,9,0,0,0,0,9},
		{9,0,9,9,9,0,9,9,9,9},
		{9,9,0,9,9,0,0,0,0,9},
		{9,0,9,9,0,9,9,9,0,9},
		{9,9,9,9,9,9,9,9,9,9}
	};
	
	public void showMaze(){
		for(int i=0; i<10; i++){
			for(int j=0; j<10; j++){
				if(maze[i][j] == 9){
					System.out.println("â–“");
				}
				
			}
		}
	}
	
	
}
