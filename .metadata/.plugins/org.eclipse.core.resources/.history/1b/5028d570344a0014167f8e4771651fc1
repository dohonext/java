		if (stationName.equals("Chuncheon"))
		{
			return 31;
		}
		else if(stationName.equals("Daejeon"))
		{
			return 15;
		}
		else if(stationName.equals("Gwangju"))
		{
			return 18;
		}
		else if (stationName.equals("Asan"))
		{
			return 43;
		}
		else if (stationName.equals("Seoul"))
		{
			return 47;
		}
		else if (stationName.equals("Wonju"))
