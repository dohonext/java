package inheritance;

public class Manager extends Employee{
	public String department;
	
	public Manager(){
		//super(); //컴파일러가 자동으로 불러줌. this가 나 super는 parent.  
		System.out.println("Manager() called");
	}
	
	public Manager(String name, double salary){
		super(name, salary);
		department = "Personal";
	}
	
	
}
